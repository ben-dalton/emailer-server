{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parser": "typescript-eslint-parser",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true
    }
  },
  "extends": "airbnb",
  "rules": {
    "comma-dangle": 1,
    "no-debugger": 0,
    "import/prefer-default-export": 0,
    "max-len": [1, 80]
  }
}
